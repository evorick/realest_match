<p id="notice"><%= notice %></p>

<h1>Listing Listers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone number</th>
      <th>Email</th>
      <th>Number bedrooms</th>
      <th>Number bathrooms</th>
      <th>City</th>
      <th>State</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listers.each do |lister| %>
      <tr>
        <td><%= lister.first_name %></td>
        <td><%= lister.last_name %></td>
        <td><%= lister.phone_number %></td>
        <td><%= lister.email %></td>
        <td><%= lister.number_bedrooms %></td>
        <td><%= lister.number_bathrooms %></td>
        <td><%= lister.city %></td>
        <td><%= lister.state %></td>
        <td><%= lister.price %></td>
        <td><%= link_to 'Show', lister %></td>
        <td><%= link_to 'Edit', edit_lister_path(lister) %></td>
        <td><%= link_to 'Destroy', lister, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lister', new_lister_path %>
